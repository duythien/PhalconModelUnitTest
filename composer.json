{
    "name": "silverbadge/PhalconModelUnitTest",
    "description": "An example of how to unit test phalcon model static methods",
    "keywords": ["unitest", "phpunit", "phalcon", "model"],
    "type": "library",
    "license": "Apache2",
    "homepage": "https://github.com/silverbadge/PhalconModelUnitTest",
    "authors": [
        {
            "name": "Rami",
            "homepage": "http://www.rami.me.uk"
        }
    ],
    "autoload": {
        "psr-0": {
            "Silverbadge": "src/",
            "Tests":"tests"
        }
    },
    "config": {
        "vendor-dir": "vendor",
        "bin-dir": "vendor/bin"
    },
    "require": {
        "php": ">=5.3.0",
        "ext-phalcon": ">=1.2.3,<2"
    },
    "require-dev": {
        "phpunit/phpunit": ">=3.7.0",
        "phpunit/php-code-coverage": ">=1.2.10,<1.3.0",
        "satooshi/php-coveralls": "dev-master",
        "squizlabs/php_codesniffer": "1.*"
    }
}

